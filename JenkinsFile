pipeline {
    agent any

    stages {
        stage('Comando no Windows2k2') {
            agent {
                label 'windows'
            }
            steps {
                bat 'echo %COMPUTERNAME%'
                bat 'ipconfig'
            }
        }
        stage('Comando no Linux') {
            agent {
                label 'linux'
            }
            steps {
                sh 'echo $HOSTNAME'
                sh 'ifconfig'
            }

        }
    }
}